<div class="form-wrapper-signup">
      <h1 style="color: #3d3d3e">SIGN UP</h1>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>
      <div class="form-item">
        <%= f.text_field :name, aotofocus: true, placeholder:"氏名(漢字)" %>
      </div>
        <div class="form-item">
          <%= f.text_field :name_kana, autofocus: true, placeholder:"氏名(フリガナ)" %>
        </div>
        <div class="form-item">
          <%= f.text_field :postal_code, autofocus: true, placeholder:"郵便番号 : 1234567(ハイフンなし)" %>
        </div>
        <div class="form-item">
          <%= f.text_field :street_address, autofocus: true, placeholder:"住所 : ※集合住宅にお住いの方は建物名・部屋峰号を必ず入力して下さい" %>
        </div>
        <div class="form-item">
          <%= f.telephone_field :phone_number, autofocus:true, placeholder:"電話番号 : 09012345678(ハイフンなし)" %>
        </div>
        <div class="form-item">
          <%= f.email_field :email, autofocus: true, placeholder:"メールアドレス" %>
        </div>
        <div class="form-item">
          <%= f.password_field :password, autocomplete:"off", placeholder:"パスワード(6文字以上)" %>
        </div>
        <div class="button-panel">
          <%= f.submit "Sign up", class: "button" %>
        </div>
      <% end %>
  <div class="form-footer">
  </div>
</div>