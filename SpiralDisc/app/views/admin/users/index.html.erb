	<div style=" padding-top: 80px" class="container clearfix">
	<%= form_tag admin_users_path, :method => 'get', class:"input-group input-group-sm search pull-right" do %>
		<span class="input-group-addon" style="border-top-left-radius: 3px; border-bottom-left-radius: 3px;">キーワード</span>
		<%= text_field_tag :search, params[:search], class:"form-control" %>
		<span class="input-group-btn">
			<%= submit_tag "検索", class:"btn btn-default", :name => nil %>
		</span>
	<% end %>
	</div>
	<div class="panel panel-default container user-list" style="background-color: #f5f5f5;">
		<div class="panel-heading">
			<h3>ユーザ一覧</h3>
		</div>
		<div class="panel-body" style="background-color: white;">
			<table class="table">
				<thead>
					<tr>
						<th class="col-md-3">名前</th>
						<th class="col-md-3">メールアドレス</th>
						<th class="col-md-3">住所</th>
						<th class="col-md-3">最終ログイン日時</th>
					</tr>
				</thead>
				<tbody>
					<% @users.each do |user| %>
						<tr>
							<th><%= link_to admin_user_path(user) do %><%= user.name %><% end %></th>
							<td><%= user.email %></td>
							<td><%= user.street_address %></td>
							<td><%= user.last_sign_in_at %></td>
						</tr>
					<% end %>
				</tbody>
			</table>
			<nav>
				<%= paginate @users, class: "pagenate" %>
			</nav>
		</div>
		<div class="panel-footer" style="background-color: #f5f5f5;">
		</div>
	</div>